<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="nba.schema.hibernate.Coach_stats" table="coach_stats">
        <id name="coach_stats_id">
            <generator class="increment"/>
        </id>
        <property name="experience"/>
        <property name="wins_rat"/>
        <property name="offensive"/>
        <property name="defensive"/>
        <property name="champs_rat"/>
        <property name="coach_id"/>
    </class>
</hibernate-mapping>